{
  "name": "tinejs",
  "version": "0.1.38",
  "description": "Yet another typescript framework",
  "scripts": {
    "test": "jest",
    "prettier": "prettier -w src",
    "lint": "eslint --ext .ts,.js . && prettier -c src",
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satoshi-cyber/tinejs.git"
  },
  "author": "Bersen Pajaziti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satoshi-cyber/tinejs/issues"
  },
  "homepage": "https://www.tinelang.org",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jest": "^29.5.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0"
  }
}
